{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 71,
      "minLength": 3
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "moisturizers",
        "foundations",
        "serums",
        "Lips",
        "Eyes"
      ]
    },
    "price": {
      "required": true,
      "type": "integer"
    }
  }
}
